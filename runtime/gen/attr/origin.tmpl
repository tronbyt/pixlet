{{ if not .IsReadOnly }}
w.starlark{{ .GoName }} = {{ .StarlarkName }}
if {{ .StarlarkName }} == nil {
	w.{{ .GoPath }} = animation.DefaultOrigin
} else if val, ok := {{ .StarlarkName }}.(*Origin); ok {
	w.{{ .GoPath }} = val.Origin
} else {
	return nil, fmt.Errorf("invalid type for origin: %s (expected Origin)", {{ .StarlarkName }}.Type())
}
{{- end }}
