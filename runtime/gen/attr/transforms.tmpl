{{ if not .IsReadOnly }}
w.starlark{{ .GoName }} = {{ .StarlarkName }}
for i := range {{ .StarlarkName }}.Len() {
	if val, ok := {{ .StarlarkName }}.Index(i).(transformUnwrapper); ok {
		w.{{ .GoPath }} = append(w.{{ .GoPath }}, val.AsAnimationTransform())
	} else {
		return nil, fmt.Errorf("expected transform, but got '%s'", {{ .StarlarkName }}.Index(i).Type())
	}
}
{{- end }}
