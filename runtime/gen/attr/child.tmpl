{{ if not .IsReadOnly }}
if {{ .StarlarkName }} != nil {
	{{ .StarlarkName }}Widget, ok := {{ .StarlarkName }}.({{ .GoWidgetName }})
	if !ok {
		return nil, fmt.Errorf(
			"invalid type for {{ .StarlarkName }}: %s (expected Widget)",
			{{ .StarlarkName }}.Type(),
		)
	}
	w.{{ .GoPath }} = {{ .StarlarkName }}Widget.AsRenderWidget()
	w.starlark{{ .GoName }} = {{ .StarlarkName }}
}
{{- end }}
