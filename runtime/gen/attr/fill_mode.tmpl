{{ if not .IsReadOnly }}
w.starlark{{ .GoName }} = {{ .StarlarkName }}
switch {{ .StarlarkName }} {
case "forwards":
	w.{{ .GoPath }} = animation.FillModeForwards{}
case "backwards":
	w.{{ .GoPath }} = animation.FillModeBackwards{}
case "":
	w.{{ .GoPath }} = animation.DefaultFillMode
default:
	return nil, fmt.Errorf("invalid type for fill_mode: %s (expected 'forwards' or 'backwards')", {{ .StarlarkName }}.Type())
}
{{- end }}
