{{ if not .IsReadOnly }}
w.starlark{{ .GoName }} = {{ .StarlarkName }}
switch {{ .StarlarkName }} {
case "round":
	w.{{ .GoPath }} = animation.Round{}
case "floor":
	w.{{ .GoPath }} = animation.RoundFloor{}
case "ceil":
	w.{{ .GoPath }} = animation.RoundCeil{}
case "none":
	w.{{ .GoPath }} = animation.RoundNone{}
case "":
	w.{{ .GoPath }} = animation.DefaultRounding
default:
	return nil, fmt.Errorf("invalid type for rounding: %s (expected 'round', 'floor', 'ceil' or 'none')", {{ .StarlarkName }}.Type())
}
{{- end }}
